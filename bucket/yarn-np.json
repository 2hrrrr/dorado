{
    "homepage": "https://yarnpkg.com/",
    "description": "Fast, reliable, and secure dependency management.",
    "license": "BSD-2-Clause",
    "version": "1.19.2",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": "dorado/nvm-windows"
    },
    "url": "https://yarnpkg.com/downloads/1.19.2/yarn-1.19.2.msi",
    "hash": "66eb081887c000a7e7facfd2c0f1937f118fdb5c69580cf534640754b1d79e86",
    "installer": {
        "script": "add_first_in_path \"$env:LOCALAPPDATA\\Yarn\\bin\" $global"

{
    "homepage": "https://github.com/evanw/esbuild",
    "description": "An extremely fast JavaScript bundler and minifier",
    "version": "0.8.52",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://registry.yarnpkg.com/esbuild-windows-64/-/esbuild-windows-64-0.8.52.tgz",
            "hash": "sha1:0a46906b33f97f1f702d61e2e72ea2fa3bf4ab8d"
        },
        "32bit": {

    "hash": "87a962005981f91ff4c854ab9ea057b1748188dde5645a04fde17b86bcd754e9",
    "extract_dir": "components-16.1",
    "pre_install": [
        "Expand-7ZipArchive \"$dir\\binutils-2.31.1.7z\" \"$dir\"",

    "architecture": {
        "64bit": {
            "url": "https://download.mozilla.org/?product=firefox-63.0.3-SSL&os=win64&lang=zh-CN#/firefox.7z",
            "hash": "e87cdf6773e2b7dbe97c69749df971e938c5aab8637a247d05dfc7149d9f33fd"
        },
        "32bit": {
            "url": "https://download.mozilla.org/?product=firefox-63.0.3-SSL&os=win&lang=zh-CN#/firefox.7z",
            "hash": "9d7996dc2b11e209d691178a87ed17828bb63e372dd356515ea4eda46e01ece9"
        }
    },
    "extract_dir": "core",
    "bin": "firefox.exe",
    "shortcuts": [
        [
            "firefox.exe",
            "Mozilla Firefox"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/zh-CN/firefox/all/",
        "re": "data-latest-firefox=\"([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win64&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win64/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            },
            "32bit": {
                "url": "https://download.mozilla.org/?product=firefox-$version-SSL&os=win&lang=zh-CN#/firefox.7z",
                "hash": {
                    "find": "([a-fA-F0-9]{64})\\s+(?:win32/zh-CN/Firefox Setup ([\\d.]+).exe)",
                    "url": "https://download-installer.cdn.mozilla.net/pub/firefox/releases/$version/SHA256SUMS"
                }
            }
        }
    }
}
